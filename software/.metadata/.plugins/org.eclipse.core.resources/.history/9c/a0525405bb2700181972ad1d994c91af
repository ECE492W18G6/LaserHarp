#include  <cpu.h>
#include  <cpu_core.h>

#include  <os.h>
#include  <hps.h>
#include  <socal.h>
#include  <hwlib.h>
#include <math.h>


void writeFreqToSynthesizer(void * diode, float frequency) {
	alt_write_word(diode, frequency);
}

INT32S readFromSythesizer(void * diode, int enabled) {
	if(enabled) {
		return (alt_read_word(diode) >> SYNTH_OFFSET);
	}
	return 0;
}



